package com.bench;

public class MenuItems 
{
	private String menuItemType;
	private String menuItemName;
	private double menuItemPrice;
	
	public MenuItems(String menuItemType, String menuItemName, double menuItemPrice) {
		super();
		this.menuItemType = menuItemType;
		this.menuItemName = menuItemName;
		this.menuItemPrice = menuItemPrice;
	}
	
	public MenuItems() {
		super();
	}

	public String getMenuItemType() {
		return menuItemType;
	}

	public void setMenuItemType(String menuItemType) {
		this.menuItemType = menuItemType;
	}

	public String getMenuItemName() {
		return menuItemName;
	}

	public void setMenuItemName(String menuItemName) {
		this.menuItemName = menuItemName;
	}

	public double getMenuItemPrice() {
		return menuItemPrice;
	}

	public void setMenuItemPrice(double menuItemPrice) {
		this.menuItemPrice = menuItemPrice;
	}

	@Override
	public String toString() {
		return "MenuItems [menuItemType=" + menuItemType + ", menuItemName=" + menuItemName + ", menuItemPrice="
				+ menuItemPrice + "]";
	}
	
	


}
